<p id="notice"><%= notice %></p>

<h1>Divisas</h1>

<div class="table-responsive">
<table id="tblproperties" class="table">
  <thead>
    <tr>
      <th>Año</th>
      <th>Mes</th>
      <th>Dia</th>
      <th>Valor</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @divisas.each do |divisa| %>
      <tr>
        <td><%= divisa.año %></td>
        <td><%= divisa.mes %></td>
        <td><%= divisa.dia %></td>
        <td><%= divisa.valor %></td>
        <td><%= divisa.tipo %></td>
        <td><%= link_to 'Show', divisa %></td>
        <td><%= link_to 'Edit', edit_divisa_path(divisa) %></td>
        <td><%= link_to 'Destroy', divisa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<script>
  $(document).ready( function () {
      $('#tblproperties').DataTable({"sPaginationType": "full_numbers"});
  } );
</script>
</div>

<br>

<%= link_to 'New Divisa', new_divisa_path %>
